{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "curly": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-shadowed-variable": false,
    "no-unused-expression": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "no-console": false,
    "no-empty": false,
    "whitespace": false,
    "object-literal-sort-keys": false,
    "max-line-length": false,
    "interface-name": false,
    "max-classes-per-file": false,
    "import-spacing": false,
    "ordered-imports": false
  },
  "rulesDirectory": []
}